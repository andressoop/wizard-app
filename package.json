{
  "name": "wizard-app",
  "description": " Project management software for freelancers & small teams",
  "productName": "Wizard",
  "author": "Andre Pukk",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest --no-cache",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "electron-builder install-app-deps"
  },
  "dependencies": {
    "@dicebear/avatars": "^3.0.6",
    "@dicebear/avatars-gridy-sprites": "^1.0.11",
    "@fortawesome/fontawesome-free": "^5.8.1",
    "ajv": "^6.9.1",
    "bootstrap": "^4.3.1",
    "electron-builder": "^20.40.2",
    "express": "^4.16.4",
    "firebase": "^5.11.1",
    "jquery": "^3.4.1",
    "lodash": "^4.17.11",
    "moment": "^2.24.0",
    "popper.js": "^1.15.0",
    "sweet-modal-vue": "^2.0.0",
    "sweetalert2": "^8.8.5",
    "vue": "^2.5.22",
    "vue-multiselect": "^2.1.6",
    "vue-router": "^3.0.5",
    "vue-sidebar-menu": "^3.7.0",
    "vue2-transitions": "^0.2.3",
    "vuedraggable": "^2.20.0",
    "vuejs-datepicker": "^1.5.4",
    "vuelidate": "^0.7.4",
    "vuex": "^3.1.0",
    "vuex-persistedstate": "^2.5.4"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.5.1",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-core": "7.0.0-bridge.0",
    "babel-jest": "^24.7.1",
    "babel-preset-env": "^1.7.0",
    "electron": "^4.1.4",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.7.1",
    "node-sass": "^4.9.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-electron-builder": "^1.1.0",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.5.21"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    }
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "jest": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "main": "background.js"
}
